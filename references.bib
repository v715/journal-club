	urldate = {2019-01-11},
	journal = {Scientific Reports},
	author = {Smith, Dylan B. and Bernhardt, Galina and Raine, Nigel E. and Abel, Richard L. and Sykes, Dan and Ahmed, Farah and Pedroso, Inti and Gill, Richard J.},
