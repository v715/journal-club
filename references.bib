	keywords = {Animals, Humans, Male, Rats, Anti-HIV Agents, Benzoxazines, Hepatocytes, Lipid Metabolism, Lipids, Mitochondria, Liver, Oxygen Consumption, Protein Kinases, Rats, Sprague-Dawley},
	pages = {115--125},
