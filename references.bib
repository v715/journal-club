	pmid = {15168736},
	keywords = {HIV Infections, Humans, Anti-HIV Agents, Reverse Transcriptase Inhibitors, Drug Therapy, Combination, HIV-1, Africa, Drug Resistance, Viral},
	pages = {4--12},
}

